<section class="perks" id="perks">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="center-title">
                    <h1>perks</h1>
                </div>
            </div>
        </div>
        <div class="row perks-wrap">
            {{#each list_perks}}
            <div class="col-md-4">
                <div class="row perks-element">
                    <div class="perks-block">
                        <img src="{{#unless isPageHome}}..{{/unless}}/{{this.image_url}}" alt="" />
                        <p><b>{{benefit_description}}</b></p>
                        <p>{{platform_description}}</p>
                        <div class="add-btn perks-btn_wrap">
                            <a href="{{this.affiliate_url}}" class="btn btn-primary">
                                Get now
                            </a>
                        </div>
                    </div>
                </div>
            </div>

            {{/each}}

        </div>
    </div>
    <div class="perks-bg perks-bg_secong">
        <img src="{{#unless isPageHome}}..{{/unless}}/images/bg-image4.png" class="img-fluid" alt="#" />
    </div>
</section>